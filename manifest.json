{
	"name": "Crabada click bot",
	"version": "0.4.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["https://play.crabada.com/*"],
			"js": ["content.js"]
		}
	],
	"icons": {
		"16": "logo48.png",
		"48": "logo48.png"
	},
	"action": {
		"default_popup": "popup/popup.html",
		"default_title": "Crabada click bot"
	}
}
