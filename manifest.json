{
	"name": "Crabada clicker",
	"version": "1.0.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["https://play.crabada.com/*", "https://idle.crabada.com/mine"],
			"js": ["content.js"]
		}
	],
	"icons": {
		"16": "logo48.png",
		"48": "logo48.png"
	},
	"action": {
		"default_popup": "popup/popup.html",
		"default_title": "Crabada clicker"
	}
}
